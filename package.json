{
  "name": "MQTTBox",
  "version": "0.2.2",
  "description": "MQTTBox - Developers helper program to create and test MQTT connectivity protocol.",
  "repository": {
    "type": "git",
    "url": "git@github.com:workswithweb/MQTTBox.git"
  },
  "main": "main.js",
  "scripts": {
    "start": "gulp",
    "electron": "electron .",
    "build": "gulp build",
    "icons": "bash scripts/make-icns.sh",
    "pack:mac": "electron-packager . MQTTBox --platform=darwin --arch=x64 --overwrite --out=dist --app-version=$npm_package_version --icon=src/www/images/icon.icns",
    "dmg:mac": "bash -c 'set -e; APP=\"dist/MQTTBox-darwin-x64/MQTTBox.app\"; VER=\"$npm_package_version\"; OUT=\"dist/MQTTBox-$VER.dmg\"; TMP=\"dist/dmg-tmp\"; rm -rf \"$TMP\"; mkdir -p \"$TMP\"; cp -R \"$APP\" \"$TMP/\"; ln -s /Applications \"$TMP/Applications\"; hdiutil create -volname \"MQTTBox $VER\" -srcfolder \"$TMP\" -ov -format UDZO \"$OUT\"'"
  },
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babelify": "7.3.0",
    "browser-sync": "2.18.2",
    "browserify": "13.1.1",
    "electron": "^1.4.11",
    "gulp": "3.9.1",
    "gulp-notify": "2.2.0",
    "fancy-log": "^2.0.0",
    "ansi-colors": "^4.1.3",
    "pretty-hrtime": "1.0.3",
    "require-dir": "0.3.1",
    "underscore": "1.8.3",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.7.0",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-router": "3.0.0",
    "flux": "3.1.0",
    "uuid": "^9.0.0",
    "localforage": "^1.10.0",
    "mqtt": "3.0.0",
    "react-copy-to-clipboard": "4.2.3",
    "qlobber": "0.7.0",
    "lodash": "^4.17.21",
    "moment": "2.17.1",
    "fixed-data-table": "0.6.3",
    "recharts": "0.19.0",
    "getmac": "1.2.1",
    "compare-versions": "3.0.0"
  }
}
